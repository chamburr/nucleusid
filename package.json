    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.17.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "nuxt": "^2.15.8",
    "v-click-outside": "^3.1.2",
    "vue2-transitions": "^0.3.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
